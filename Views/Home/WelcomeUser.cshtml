@model CoffeeshopUser

@{
    <div>
        <h2>Welcome, @Model.FirstName!</h2>
        <br />
        <h3>Your Account Details:</h3>
        <p>Email: @Model.Email</p>
        <p>First Name: @Model.FirstName</p>
        <p>Last Name: @Model.LastName</p>
    </div>
}